<?xml version="1.0" encoding="utf-8"?>
<extension type="module" version="1.6" method="upgrade">
	<name>Bigbluebutton</name>
	<creationDate>2018-07-21</creationDate>
	<author>Jibon Lawrence Costa</author>
	<authorEmail>jiboncosta57@gmail.com</authorEmail>
	<authorUrl>www.hoicoimasti.com</authorUrl>
	<copyright>Copyright (C) 2018  hoicoimasti.com. All rights reserved.</copyright>	
	<license>MIT</license>
	<version>2.0.1</version>
	<description></description>
	<files>
		<filename module="mod_bigbluebutton">mod_bigbluebutton.php</filename>
		<folder>tmpl</folder>
		<filename>index.html</filename>
		<filename>mod_bigbluebutton.xml</filename>
	</files>
	<languages>
		<language tag="en-GB">language/en-GB.mod_bigbluebutton.ini</language>
	</languages>
	<params>
		<param name="classname" type="text" default="" label="Class Name" description="Class Name Description" />
	</params>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field
					name="showCat"
					type="radio"
					label="Enable Category"
					description="Show Category filter"
					class="btn-group btn-group-yesno"
					default="0"
					filter="integer">
						<option value="1">JYES</option>
						<option value="0">JNO</option>
				</field>
				<field name="classname" type="text" default="" label="Class Name" description="Class Name Description" />
			</fieldset>
		</fields>
	</config>			
</extension>
